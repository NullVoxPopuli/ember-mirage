{
  "name": "ember-mirage",
  "version": "0.1.0",
  "description": "Enable MirageJS in Ember.",
  "keywords": [
    "ember-addon"
  ],
  "repository": "",
  "license": "MIT",
  "author": "Brian Gantzler",
  "files": [
    "addon",
    "addon-test-support"
  ],
  "scripts": {
    "lint": "pnpm -w exec turbo _:lint --filter ember-mirage",
    "lint:fix": "pnpm -w exec turbo _:lint:fix --filter ember-mirage",
    "lint:js": "eslint .",
    "lint:js:fix": "pnpm lint:js --fix",
    "lint:hbs": "ember-template-lint . --no-error-on-unmatched-pattern",
    "lint:hbs:fix": "pnpm lint:hbs --fix",
    "lint:prettier:fix": "prettier -w .",
    "lint:prettier": "prettier -c .",
    "start": "ember serve",
    "test": "ember test",
    "test:ember": "ember test",
    "test:all": "ember try:each"
  },
  "dependencies": {
    "@embroider/macros": "^1.12.0",
    "ember-auto-import": "^2.6.3",
    "ember-cli-babel": "^7.26.11",
    "ember-cli-htmlbars": "^6.2.0",
    "ember-inflector": "^4.0.2",
    "lodash-es": "^4.17.21",
    "miragejs": "^0.1.47"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.22.5",
    "@ember/optional-features": "^2.0.0",
    "@embroider/test-setup": "^3.0.1",
    "@nullvoxpopuli/eslint-configs": "^3.1.4",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~5.0.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-qunit": "^7.0.0",
    "ember-resolver": "^10.1.1",
    "ember-source": "~5.0.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^5.11.0",
    "ember-try": "^2.0.0",
    "eslint": "^8.43.0",
    "eslint-plugin-ember": "^11.9.0",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-qunit": "^8.0.0",
    "loader.js": "^4.7.0",
    "prettier": "^2.8.8",
    "qunit-dom": "^2.0.0",
    "webpack": "^5.88.1"
  },
  "engines": {
    "node": ">= 18"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "before": [
      "ember-cli-babel"
    ],
    "after": [
      "ember-qunit"
    ]
  },
  "volta": {
    "extends": "../../package.json"
  }
}
